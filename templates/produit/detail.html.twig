{% extends 'base.html.twig' %}

{% block title %}Détail{% endblock %}

{% block body %}
<h1>Détail Produit</h1>
<div class="container">
  <table class="table">
 
  <tbody>
    
    <tr>
      <th scope="row">ID</th>
      <td>{{ produit.id }}</td>
       
    </tr>
  <tr>
      <th scope="row">Nom</th>
      <td>{{ produit.nom }}</td>
       
    </tr>
   <tr>
      <th scope="row">PRIX</th>
     
       <td>{{ produit.prix }}</td>
        
    </tr>
      
      <tr>
      <th scope="row">DESCRIPTION</th>
      
        <td>{{ produit.description }}</td>
    </tr>
 
   </tbody>
   </table>
</div>






{% endblock %}

  